{
  "name": "Doing now",
  "version": "1.0",
  "manifest_version": 2,
  "description": "Notifying what are you doing now.",
  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://*/*"],
      "js": ["js/popup.js"]
    }
  ],
  "background": {
    "scripts": ["js/lib/jquery-2.1.0.min.js", "js/background.js"]
  },
  "permissions": [
    "http://*/*", "https://*/*",
    "tabs",
    "notifications",
    "storage"
  ]
}